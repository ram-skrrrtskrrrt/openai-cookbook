{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "title": {
        "type": "user"
      },
      "path": {
        "type": "user"
      },
      "redirects": {
        "type": "array",
        "items": {
          "type": "user"
        }
      },
      "tags": {
        "type": "array",
        "items": {
          "type": "user"
        }
      },
      "authors": {
        "type": "array",
        "items": {
          "type": "user"
        }
      },
      "date": {
        "type": "user",
        "format": "date"
      }
    },
    "required": ["title", "path", "tags", "authors"],
    "additionalProperties": true
  }
}
